{
  "name": "@n4s/xcomponent",
  "version": "3.4.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "ladle dev",
    "build": "pnpm build:clean; pnpm build:cjs; pnpm build:mjs;",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "build:cjs:watch": "tsc -p tsconfig.cjs.json --watch",
    "build:mjs:watch": "tsc -p tsconfig.mjs.json --watch",
    "build:watch": "pnpm build:clean; pnpm run '/(cjs|mjs):watch$/'",
    "build:clean": "rm -rf ./x/*",
    "test": "vitest dev -r ./src"
  },
  "main": "./x/cjs/index.js",
  "module": "./x/mjs/index.js",
  "types": "./x/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./x/mjs/index.js",
      "require": "./x/cjs/index.js"
    },
    "./*": {
      "import": "./x/mjs/*.js",
      "require": "./x/cjs/*.js"
    }
  },
  "files": [
    "x/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nfour/xcomponent"
  },
  "devDependencies": {
    "@emotion/react": "^11.13.3",
    "@ladle/react": "^4.1.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/ui": "^2.1.2",
    "eslint": "8.43.0",
    "eslint-config-nfour": "^3.5.0",
    "fetch-mock": "^11.1.3",
    "history": "^5.3.0",
    "jsdom": "^25.0.1",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "~5.7.3",
    "vite": "^6.1.0",
    "zod": "^3.24.1",
    "vitest": "^2.1.2",
    "xroute": "^15.8.1"
  },
  "peerDependencies": {
    "mobx": ">= 6",
    "mobx-react-lite": ">= 4",
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "microdiff": "^1.4.0",
    "remeda": "^2.14.0"
  }
}